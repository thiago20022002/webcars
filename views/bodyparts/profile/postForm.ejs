<form class="form-horizontal" ng-submit="sendFeedbackPost()">
    <fieldset>
        <!-- Form Name -->
        <legend>Leave your feedback</legend>

        <div class="form-group">
            <label class="col-md-2 control-label" for="textarea">Feedback</label>
            <div class="col-md-10">
                <textarea class="form-control" id="textarea" ng-model="feedData.feedback"></textarea>
            </div>
        </div>

        <!-- Button -->
        <div class="form-group">
            <div class="col-md-12">  
               
             <button type="submit" class="btn btn-default pull-right"><i class="fa fa-comment"></i>Feedback</button>
            </div>
        </div>
    </fieldset>
</form>
