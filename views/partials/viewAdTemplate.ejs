<div class="container col-md-12"> 
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <fieldset>
            <legend>
                {{make}}
                <span class="model-tag">{{model}}</span>
                <span class="pull-right">${{price}}</span>
            </legend>

            <div class="item active">
                <img class="img-responsive center-block" src="{{urlImage}}">
            </div>

            <ul class="col-md-12 list-group">
                <li class="list-group-item col-md-12" >
                    <div class="col-md-9">
                        <a href="/profile/{{username}}">
                            <div class="col-md-3 right-line">
                                <img class="round_img" src="{{sellerImg}}" alt="...">
                            </div> 
                            <div class="col-md-9">
                                <span class="user-name">{{username}}</span>
                            </div>
                        </a>
                    </div>
                    <div class="col-md-3">
                        <h3>
                            <span>Views:{{views}}</span>
                        </h3>
                    </div>
                </li>
            </ul>
            <div class="col-md-12">
                <ul class="list-inline">
                    <li class="list-group-item"><h4>Year: {{year}}</h4></li>
                    <li class="list-group-item"><h4>Make: {{make}}</h4></li>
                    <li class="list-group-item"><h4>Model: {{model}}</h4></li>
                    <li class="list-group-item"><h4>Location: {{location}}</h4></li>
                </ul>

            </div>
            <div class="col-md-12 description-box">
                {{description}}
            </div>

        </fieldset>
    </div>

</div>